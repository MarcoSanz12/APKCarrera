<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CheckBox" parent="Widget.Material3.CompoundButton.CheckBox">
        <item name="android:textSize">12sp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:maxLines">3</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:layout_marginStart">3dp</item>
        <item name="buttonIconTint">@color/switch_checkbox_selector</item>
        <item name="buttonTint">@color/white</item>
        <item name="buttonCompat">@drawable/check_box</item>
        <item name="buttonIcon">@drawable/check</item>
    </style>
</resources>