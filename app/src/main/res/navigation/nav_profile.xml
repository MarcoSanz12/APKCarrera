<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_profile"
    app:startDestination="@id/fragmentProfile">

    <argument
        android:name="uid"
        app:argType="string"
        app:nullable="true"
        android:defaultValue="@null"/>

    <action android:id="@+id/action_global_navFeed"
        app:destination="@+id/nav_feed">
        <argument
            android:name="uid"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
    </action>


    <fragment
        android:id="@+id/fragmentProfile"
        android:name="com.gf.apkcarrera.features.f5_profile.fragments.ProfileFragment"
        android:label="@string/bnv_profile"
        tools:layout="@layout/frg05_profile">
        <action
            android:id="@+id/action_fragmentProfile_to_statFragment"
            app:destination="@id/statFragment" />
    </fragment>
    <fragment
        android:id="@+id/statFragment"
        android:name="com.gf.apkcarrera.features.f5_profile.fragments.statistics.StatFragment"
        android:label="@string/fragment_estadisticas"
        tools:layout="@layout/frg05_profile_stats"/>
    <fragment
        android:id="@+id/statDataFragment"
        android:name="com.gf.apkcarrera.features.f5_profile.fragments.statistics.StatDataFragment"
        android:label="@string/fragment_estadisticas"
        tools:layout="@layout/frg05_profile_stats_data"/>

</navigation>